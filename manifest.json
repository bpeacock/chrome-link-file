{
    "name": "Chrome Link File",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Creates and downloads an html file that links to a given website.",
    "icons": {
        "19": "images/icon_19.png",
        "38": "images/icon_38.png",
        "128": "images/icon_128.png"
    },
    "browser_action": {
        "default_title": "Create Link File",
        "default_icon": {
            "19": "images/icon_19.png",
            "38": "images/icon_38.png"
        },
        "default_popup": "html/main.html"
    },
    "permissions": [
      "tabs",
      "downloads"
    ],
    "options_page": "html/options.html",
    "homepage_url": "http://yourdomainhere.com/"
}